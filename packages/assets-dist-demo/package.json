{
  "name": "foxpage-component-demo-assets-dist-demo",
  "version": "0.0.0",
  "description": "foxpage component",
  "keywords": [
    "foxpage",
    "component"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:es && npm run build:foxpage",
    "build:foxpage": "foxpage-cmpt build --foxpage --file-hash",
    "build:lib": "foxpage-cmpt build --lib --css-style --remove-style-import --import-index-css",
    "build:es": "foxpage-cmpt build --es-module --css-style --remove-style-import --import-index-css",
    "build:md": "foxpage-cmpt build --schema-md",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "core-js": "^3.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
    "files": [
    "lib/**",
    "es/**",
    "dist/**",
    "*.js",
    "*.d.ts",
    "schema.md"
  ]
}
