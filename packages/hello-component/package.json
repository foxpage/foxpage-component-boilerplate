{
  "name": "hello-component",
  "version": "0.0.0",
  "description": "foxpage component",
  "keywords": [
    "foxpage",
    "component"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "foxpage": {
    "name": "hello-component",
    "publishPath": "",
    "meta": {},
    "dependencies": [],
    "disableContainer": false
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:es && npm run build:foxpage",
    "build:lib": "foxpage-cmpt build --lib",
    "build:es": "foxpage-cmpt build --es-module",
    "build:umd": "foxpage-cmpt build --umd",
    "build:cjs": "foxpage-cmpt build --cjs",
    "build:foxpage": "foxpage-cmpt build --foxpage --ignore-modes editor",
    "build:md": "foxpage-cmpt build --schema-md",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "core-js": "^3.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {},
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "files": [
    "lib/**",
    "es/**",
    "umd/**",
    "cjs/**",
    "dist/**",
    "*.js",
    "*.d.ts",
    "schema.md"
  ]
}